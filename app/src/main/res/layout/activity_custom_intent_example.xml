<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".main.examples.customintentexample.CustomIntentExampleActivity"
    android:background="@color/white"
    android:orientation="vertical"
    android:gravity="center">
    
    <Button
        android:id="@+id/buttonLaunch"
        android:backgroundTint="#333333"
        android:textAllCaps="false"
        android:textSize="7pt"
        android:textColor="@color/white"
        android:text="launch the target app with intent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    
    
    <Button
        android:id="@+id/buttonLaunchWithUri"
        android:backgroundTint="#333333"
        android:textAllCaps="false"
        android:textSize="7pt"
        android:textColor="@color/white"
        android:text="launch the target app with uri and pass data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    
</LinearLayout>