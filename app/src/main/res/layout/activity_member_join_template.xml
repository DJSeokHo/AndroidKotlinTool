<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".template.memeberjoin.MemberJoinTemplateActivity"
		android:background="@color/white">
	
	<FrameLayout android:layout_width="match_parent" android:layout_height="match_parent"
	android:layout_marginTop="80dp" android:id="@+id/frameLayoutBodyContainer"/>
	
	<FrameLayout android:layout_width="match_parent" android:layout_height="80dp"
	android:id="@+id/frameLayoutNavigationContainer"/>
	
	<FrameLayout android:layout_width="match_parent" android:layout_height="match_parent"
	android:background="@color/progress_bar_bg" android:visibility="gone"
	android:id="@+id/progressFrameLayout">
		<ProgressBar android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_gravity="center"/>
	</FrameLayout>
</FrameLayout>