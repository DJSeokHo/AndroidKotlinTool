<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.swein.androidkotlintool">
    <!-- 允许程序监视、修改或放弃拨打电话 -->
    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" /> <!-- 录音权限 -->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission
        android:name="android.permission.CAPTURE_AUDIO_OUTPUT"
        tools:ignore="ProtectedPermissions" />
    <uses-permission
        android:name="android.permission.CAPTURE_SECURE_VIDEO_OUTPUT"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />
    
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".main.examples.databinding.DataBindingExampleActivity"></activity>
        <activity android:name=".main.examples.viewbinding.ViewBindingExampleActivity" />
        <activity android:name=".template.coroutine.BaseCoroutineActivity" />
        <activity android:name=".main.examples.customizecolorswitch.CustomizeColorSwitchActivity" />
        <activity android:name=".template.coroutine.CoroutineDemoActivity" />
        <activity
            android:name=".main.examples.fashionshoppingdetail.FashionShoppingDetailActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".template.mvp.mvploginexample.view.MVPLoginActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".template.blurmask.BlurMaskActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".framework.module.shcameraphoto.demo.SHCameraPhotoFragmentDemoActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTask" />
        <activity
            android:name=".framework.module.camerademo.CameraDemoActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".template.handlerthread.HandlerThreadTemplateActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".main.moduledemo.ModuleDemoActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".template.viewpagerfragment.activity.ViewPagerActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".template.bottomtab.activity.TabHostActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".template.list.SHListActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".template.memeberjoin.MemberJoinTemplateActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".splash.SplashActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
    
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:launchMode="singleTask"
            tools:ignore="Instantiatable" />
        
        <receiver
            android:name=".framework.util.shortcut.ShortcutReceiver"
            android:exported="false" />
        
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" /> <!-- FileProvider used to share photos with other apps -->
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="${applicationId}.androidx-startup"
            android:exported="false"
            tools:node="merge">
    
            <!-- This entry makes ExampleLoggerInitializer discoverable. -->
            <meta-data
                android:name="com.swein.androidkotlintool.main.examples.appstartup.AppStartUpExample"
                android:value="androidx.startup" />
    
            <!-- if you don't need -->
            <!-- <meta-data -->
            <!-- android:name="com.swein.androidkotlintool.main.examples.appstartup.AppStartUpExample" -->
            <!-- tools:node="remove" -->
            <!-- /> -->
        </provider>
    </application>

</manifest>